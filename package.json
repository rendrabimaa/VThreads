    "react-icons": "^4.12.0",
    "react-modal": "^3.16.1",
    "react-redux": "^9.0.2",
    "react-redux-loading-bar": "^5.0.5",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
